				 void Site.Changing_Site_Inspector(int record_id)
         {
         	//Redirects to the inspector assigning form
         	url = "#Form:Assign_Inspector?Site_Name=" + input.record_id;
         	openUrl(url,"popup window");
         }
				 void Site.Changing_Site_Status(int record_id)
         {
         	this_record = Add_Your_Site[ID == input.record_id];
         	//Changing status from active to inactive
         	if(this_record.Work_Status == "Active")
         	{
         		this_record.Work_Status="Inactive";
         	}
         	else
         	{
         		url = "#Form:Assign_Inspector?Site_Name=" + this_record.ID;
         		if(this_record.Select_Site_Inspector != null)
         		{
         			status = Add_Site_Inspector[ID == this_record.Select_Site_Inspector].Status;
         			//Assigning the same inspector to the site
         			if(status == "Active")
         			{
         				this_record.Work_Status="Active";
         			}
         			//Redirects to the assigning form
         			else
         			{
         				openUrl(url,"popup window");
         			}
         		}
         		else
         		{
         			openUrl(url,"popup window");
         		}
         	}
         }
				 void Site_Inspector.Changing_Inspector_Status(int record_id)
         {
         	this_record = Add_Site_Inspector[ID == input.record_id];
         	if(this_record.Status == "Active")
         	{
         		admin = Add_Site_Inspector[Email_Id == zoho.adminuserid].ID;
         		site_count = Add_Your_Site[Select_Site_Inspector == input.record_id].count(ID);
         		//Assigning admin as inspector for all the sites assigned to this inspector
         		if(site_count != 0)
         		{
         			for each  rec in Add_Your_Site[Select_Site_Inspector == input.record_id]
         			{
         				rec.Select_Site_Inspector=admin;
         			}
         		}
         		this_record.Status="Archive";
         	}
         	//Changing status to active
         	else if(this_record.Status == "Archive")
         	{
         		this_record.Status="Active";
         	}
         }
			 }
		 }




