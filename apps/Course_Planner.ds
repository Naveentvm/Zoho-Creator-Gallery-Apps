		workflow
		{
		form
		{
			Run_Scripts_On_Click_of_O as "Run Scripts - On Click of 'Ok' button"
			{
				type =  form
				form = Course_Subscription
				button = Ok
				on click
				{
					actions 
					{
					on click
					(
						courseRecord = Add_Course[ID == input.Course];
										studentId = Enroll_User[Email == zoho.loginuserid].ID;
										if(courseRecord.Course_Type == "Free")
										{
											//Add student to the corresponding course
											studentRecord = Enroll_User[Email == zoho.loginuserid];
											courseRecord = Add_Course[ID == input.Course];
											studentsList = courseRecord.Students;
											studentsList.add(studentRecord.ID);
											courseRecord.Students=studentsList;
										}
										else if(courseRecord.Course_Type == "Paid")
										{
											openUrl("#Form:Course_Payment?Course=" + input.Course + "&Student=" + studentId + "&Amount=" + courseRecord.Amount,"same window");
										}
					)
					}
				}
			}
			Run_Scripts_On_Click_of_C as "Run Scripts - On Click of 'Cancel' button"
			{
				type =  form
				form = Course_Subscription
				button = Cancel
				on click
				{
					actions 
					{
					on click
					(
						openUrl("#Script:page.refresh","same window");
					)
					}
				}
			}
			Run_Scripts_On_Click_of_Y as "Run Scripts - On Click of 'Yes' button"
			{
				type =  form
				form = Tutoring_Approval_Confirmation
				button = Yes
				on click
				{
					actions 
					{
					on click
					(
						Status = "Approved";
										requestRec = Tutoring_Request[ID == input.Tutor_Request];
										courseRecord = Add_Course[ID == requestRec.Course];
										courseRecord.Select_Tutor=requestRec.Tutor;
										tutorRec = Enroll_User[ID == requestRec.Tutor];
										sendmail
										[
											from :zoho.adminuserid
											to :tutorRec.Email
											subject :"Tutoring Request Approval Notification"
											message :("Hi " + tutorRec.Name) + ",<div><br></div><div>Your tutoring request for " + courseRecord.Course_Name + " has been approved.&nbsp;</div><div><br></div><div>Click <a href=\"" + "https://app.zohocreator.in" + zoho.appuri + "#Report:Tutoring_Requests_Tutor\"" + " target=\"_blank\">here</a> to view the details.</div><div><br></div><div>Regards,</div><div>" + zoho.adminuser + "</div>"
										]
										allRequests = Tutoring_Request[Course == requestRec.Course && Status == "Requested" && ID != input.Tutor_Request];
										for each  request in allRequests
										{
											tutorRec = Enroll_User[ID == request.Tutor];
											request.Status="Rejected";
											sendmail
											[
												from :zoho.adminuserid
												to :tutorRec.Email
												subject :"Tutoring Request Rejection Notification"
												message :("Hi " + tutorRec.Name) + ",<div><br></div><div>We regret to inform you that your request for tutoring '" + courseRecord.Course_Name + "' has been rejected by admin due to some valid reasons.</div><div><br></div><div>Click <a href='https://app.zohocreator.in" + zoho.appuri + "#Report:Tutoring_Requests_Tutor' target='_blank'>here</a> to view the status.</div><div><br></div><div>Regards,</div><div>" + zoho.adminuser + "</div><div><br></div>"
											]
										}
										openUrl("#Script:page.parent.refresh","same window");
					)
					}
				}
			}
