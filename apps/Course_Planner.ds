	section Add_Course
	{
		displayname= "Courses"
		icon = "education-hat"
		form Add_Course
		{
			displayname = "Add Course"
			success message = "New Course Added Successfully!"
			field alignment = left
			feature property = 32
			Section
			(
				type = section
	 			row = 1
	 			column = 0   
				width = medium
			)
			Tutoring_Requests
			(
				type = list	
				displayname = "Tutoring Requests"
				values  = Tutoring_Request.ID
    			bidirectional = Course
    			displayformat = [ID]
				height = 60px
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have Course_Name
			(
    			type = text
				displayname = "Course Name"
	 			row = 1
	 			column = 1   
				width = medium
			)
			unique Course_Code
			(
    			type = text
				displayname = "Course Code"
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have Category
			(
				type = picklist	
				values  = Add_Course_Category.ID
    			displayformat = [Category_Name]
				allow new entries = true
				[
					displayname = "Add New Category"
				]
				sortorder = ascending
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have Start_Date
			(
    			type = date
				displayname = "Start Date"
				alloweddays = 0,1,2,3,4,5,6
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have End_Date
			(
    			type = date
				displayname = "End Date"
				alloweddays = 0,1,2,3,4,5,6
	 			row = 1
	 			column = 1   
				width = medium
			)
			Course_Summary
			(
    			type = richtext
				displayname = "Course Summary"
				height = 200px
				toolbar = enable
				[
					style, font-color, background-color, alignment, link, table, bullets-and-numbering, 
					font-size : {1, 2, 3, 4, 5, 6, 7},
					font-family : {"Serif", "Arimo", "Ptmono", "Heuristica", "OpenSans", "Tinos", "Lato", "DejaVuSans", "ComicNeue"}
				]
	 			row = 1
	 			column = 1   
				width = medium
			)
			Course_Type
			(
				type = radiobuttons
				displayname = "Course Type"
				values = {"Free","Paid"}
				initial value = "Free"
				layout = 2
	 			row = 1
	 			column = 1   
				width = medium
			)
			Amount
			(
				type = INR
				initial value = 0.00
	 			row = 1
	 			column = 1   
				width = medium
			)
			Participants
			(
				type = radiobuttons
				values = {"Limited","Unlimited"}
				initial value = "Limited"
				layout = 2
	 			row = 1
	 			column = 1   
				width = medium
			)
			Maximum_participants_allowed
			(
				type = number
				displayname = "Maximum participants allowed"
	 			row = 1
	 			column = 1   
				width = medium
			)
			Seats_Remaining
			(
				type = number
				displayname = "Seats Remaining"
	 			row = 1
	 			column = 1   
				width = medium
			)
			Status
			(
				type = picklist
				values = {"Upcoming","Ongoing","Completed"}
				initial value = "Open"
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have Tutor1
			(
				type = radiobuttons
				displayname = "Tutor"
				values = {"Assign Now","Let tutors select the course"}
	 			row = 1
	 			column = 1   
				width = medium
			)
			Select_Tutor
			(
				type = picklist	
				displayname = "Select Tutor"
				values  = Enroll_User[Enroll_as == "Tutor"].ID
    			displayformat = [" " + Name.first_name + "-" + Email]
				sortorder = ascending
	 			row = 1
	 			column = 1   
				width = medium
			)
			Students
			(
				type = list	
				values  = Enroll_User[Enroll_as == "Student"].ID
    			displayformat = [Name.first_name + "-" + Email]
				sortorder = ascending
				height = 60px
	 			row = 1
	 			column = 1   
				width = medium
			)
			No_of_Students
			(
    			type = formula
				displayname = "No of Students"
				value =  input.Students.size()
				visibility = false
	 			row = 1
	 			column = 1   
				width = medium
			)
	
			customize
			(
				icon = "files-add"
			)
			actions
			{
				on add
				{
					submit
					(
   						type = submit
   						displayname = "Submit"
					)
					reset
					(
   						type = reset
   						displayname = "Reset"
					)
				}
				on edit
				{
					update
					(
   						type = submit
   						displayname = "Update"
					)
					cancel
					(
   						type = cancel
   						displayname = "Cancel"
					)
				}
			}
		}
