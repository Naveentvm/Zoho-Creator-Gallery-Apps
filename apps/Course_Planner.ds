			Run_Scripts_On_Always3 as "Run Scripts - On Always"
			{
				type =  form
				form = Add_Course
				record event = on add or edit

				field rules
				{
					actions  (Course_Type == "Free")
					{
						hide Amount;
					}
					actions  (Participants == "Unlimited")
					{
						hide Maximum_participants_allowed;
					}
					actions 
					{
						hide Status,Tutoring_Requests;
					}
					actions  (Tutor1 == "Assign Now")
					{
						show Select_Tutor;
					}
				}

			}
			Run_Scripts_On_Always4 as "Run Scripts - On Always"
			{
				type =  form
				form = Add_Course
				record event = on add or edit

				on validate
				{
					actions  (Course_Type == "Free")
					{
						custom deluge script
						(
											//Clearing user inputs in case of 'Free' course 
										input.Amount = null;
						)
					}
					actions  (Participants == "Unlimited")
					{
						custom deluge script
						(
											//Clearing user inputs in case of 'Unlimited' participants 
										input.Maximum_participants_allowed = null;
										input.Seats_Remaining = null;
						)
					}
					actions 
					{
						custom deluge script
						(
											//Autofill status value based on given date
										if(input.Start_Date <= zoho.currentdate && input.End_Date > zoho.currentdate)
										{
											input.Status = "Ongoing";
										}
										else if(input.Start_Date > zoho.currentdate)
										{
											input.Status = "Upcoming";
										}
										else if(input.Start_Date < zoho.currentdate && input.End_Date < zoho.currentdate)
										{
											input.Status = "Completed";
										}
										//Validating start and end date
										if(input.Start_Date > input.End_Date)
										{
											alert "Please specify the valid dates!";
											cancel submit;
										}
						)
					}
					actions  (Course_Type == "Paid")
					{
						custom deluge script
						(
											//Mandate amount field in case of paid course
										if(input.Amount <= 0 || Amount == null)
										{
											alert "Please enter valid amount for paid course!";
											cancel submit;
										}
						)
					}
					actions  (Participants == "Limited")
					{
						custom deluge script
						(
											//Mandate maximum participants field in case of limitted participants
										if(input.Maximum_participants_allowed <= 0 || input.Maximum_participants_allowed == null)
										{
											alert "Please enter valid number for 'Maximum participants allowed'!";
											cancel submit;
										}
										else
										{
											//Updating remaining seats for the course
											input.Seats_Remaining = input.Maximum_participants_allowed - input.Students.size();
										}
						)
					}
				}

			}
