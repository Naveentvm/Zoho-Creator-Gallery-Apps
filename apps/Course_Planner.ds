		displayname= "Tutoring Requests"
		icon = "shopping-list"
		form Tutoring_Request
		{
			displayname = "Tutoring Request"
			success message = "Tutor Request Added Successfully"
			field alignment = left
			feature property = 32
					hide = true
			Section
			(
				type = section
	 			row = 1
	 			column = 0   
				width = medium
			)
			Tutor
			(
				type = picklist	
				values  = Enroll_User[Enroll_as == "Tutor"].ID
    			displayformat = [Name.first_name + " - " + Email]
				sortorder = ascending
	 			row = 1
	 			column = 1   
				width = medium
			)
			Tutor_Email
			(
    			type = formula
				displayname = "Tutor Email"
				value =  input.Tutor.Email
				visibility = false
				format = plaintext
	 			row = 1
	 			column = 1   
				width = medium
			)
			Course
			(
				type = picklist	
				values  = Add_Course.ID
    			bidirectional = Tutoring_Requests
    			displayformat = [Course_Name]
				sortorder = ascending
	 			row = 1
	 			column = 1   
				width = medium
			)
			Status
			(
				type = picklist
				values = {"Requested","Approved","Rejected"}
				initial value = "Requested"
				others option = true
	 			row = 1
	 			column = 1   
				width = medium
			)
	
			customize
			(
				icon = "ui-1-bold-add"
			)
			actions
			{
				on add
				{
					submit
					(
   						type = submit
   						displayname = "Submit"
					)
					reset
					(
   						type = reset
   						displayname = "Reset"
					)
				}
				on edit
				{
					update
					(
   						type = submit
   						displayname = "Update"
					)
					cancel
					(
   						type = cancel
   						displayname = "Cancel"
					)
				}
			}
		}
		list Pending_Requests
		{
			displayName = "Pending Requests"
			show all rows from Tutoring_Request  [Status == "Requested"]  
			(
				Status
				Tutor
				Course
				custom action "Approve"
				(
	  			workflow = Approve_tutor
				column header = "Approve"
				)
				custom action "Reject"
				(
	  			workflow = Reject_Tutor
				column header = "Reject"
				)
			)
			options
			(
				icon = "ui-2-hourglass"
	 		)
			quickview
			(
				layout
				(
		 			type = -1
					datablock1
					(
			 			layout type = -1
		     			title = "Overview"
						fields
						(
							Tutor
							Course
				"Approve"
				"Reject"
						)
					)
				)

				menu
    			(
    	 			header
    	 			(
    		 			Edit 
    		 			Duplicate 
    		 			Delete 
    	 			)
							record
							(
								Edit   	   
								Duplicate   	   
								Delete   	   
    						)
    			)
    			action
    			(
							on click
							(
								View Record   	   
    						)
							on right click
							(
								Edit   	   
								Delete   	   
								Duplicate   	   
								View Record   	   
    						)
     			)
			)
			detailview
			(
				layout
				(
		 			type = 1
					datablock1
					(
			 			layout type = -2
		     			title = "Overview"
						fields
						(
							Tutor
							Course
							Status
						)
					)
				)

				menu
    			(
    	 			header
    	 			(
			 			"Approve"
			 			"Reject"
    		 			Edit 
    		 			Duplicate 
    		 			Delete 
    	 			)
    			)
			)
		}
		list All_Requests
		{
			displayName = "All Requests"
			show all rows from Tutoring_Request    
			(
				Tutor
				Course
				Status
				Tutor_Email as "Tutor Email"
			)
			options
			(
				icon = "shopping-receipt-list-43"
	 		)
			conditional formatting
			(
				"Requested Formatting"
				{
				condition = (Status == "Requested")
				fields = [Status]
				format = "color:#3599db;background-color:#ffffff;"
				}
				"Approved Formatting"
				{
				condition = (Status == "Approved")
				fields = [Status]
				format = "color:#19bc9f;"
				}
				"Rejected Formatting"
				{
				condition = (Status == "Rejected")
				fields = [Status]
				format = "color:#e64d3b;"
				}
			)
			quickview
			(
				layout
				(
		 			type = -1
					datablock1
					(
			 			layout type = -1
		     			title = "Overview"
						fields
						(
							Tutor
							Course
							Status
						)
					)
				)

				menu
    			(
    	 			header
    	 			(
    		 			Edit 
    		 			Duplicate 
    		 			Delete 
    	 			)
							record
							(
								Edit   	   
								Duplicate   	   
								Delete   	   
    						)
    			)
    			action
    			(
							on click
							(
								View Record   	   
    						)
							on right click
							(
								Edit   	   
								Delete   	   
								Duplicate   	   
								View Record   	   
    						)
     			)
			)
			detailview
			(
				layout
				(
		 			type = 1
					datablock1
					(
			 			layout type = -2
		     			title = "Overview"
						fields
						(
							Tutor
							Course
							Status
							Tutor_Email as "Tutor Email"
						)
					)
				)

				menu
    			(
    	 			header
    	 			(
    		 			Edit 
    		 			Duplicate 
    		 			Delete 
    	 			)
    			)
			)
		}
	}
