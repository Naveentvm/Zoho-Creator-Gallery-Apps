				 float newOrder.calcGrandTotal(float subTotal, float tax, float discount)
         {
         	subTotalVal = ifnull(subTotal,0.00);
         	taxVal = ifnull(tax,0.00);
         	discountVal = ifnull(discount,0.00);
         	getDiscount = (subTotalVal * discountVal) / 100;
         	getTax = ((subTotalVal - getDiscount) * taxVal) / 100;
         	returnTotal = subTotalVal - getDiscount + getTax;
         	return returnTotal;
         }
				 void uinames.addUser()
         {
         	resp = getUrl("https://uinames.com/api/?amount=32&region=united states&ext").toJSONList();
         	getAllRec = Add_Sales_Rep[ID != 0].ID.getAll();
         	getTeam = (Add_Team[ID != 0].ID sort by Team_Name).getAll();
         	cnt = 0;
         	for each  eachresp in resp
         	{
         		testmap = eachresp.toMap();
         		n = testmap.get("name");
         		sn = testmap.get("surname");
         		num = testmap.get("phone").toString().remove("(").remove(")").remove(" ").trim();
         		email = testmap.get("email");
         		reg = testmap.get("region");
         		gen = testmap.get("gender");
         		birthdayMap = testmap.get("birthday").toMap();
         		dmyFormat = birthdayMap.get("dmy").toDate();
         		pic = testmap.get("photo");
         		if(gen == "male")
         		{
         			gen = "Male";
         		}
         		else
         		{
         			gen = "Female";
         		}
         		teamIndex = if(getTeam.size() <= cnt,cnt % getTeam.size(),cnt);
         		curTeam = getTeam.get(teamIndex);
         		curRec = getAllRec.get(cnt);
         		getRec = Add_Sales_Rep[ID == curRec];
         		getRec.Name.first_name=n.toString();
         		getRec.Name.last_name=sn.toString();
         		getRec.Email=email;
         		getRec.Gender=gen;
         		getRec.Phone_Number="+1" + num.toString();
         		getRec.Team=curTeam;
         		getRec.Image="<img src='" + pic + "'>";
         		cnt = cnt + 1;
         	}
         }
			 }
