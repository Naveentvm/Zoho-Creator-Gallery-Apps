		functions
		{
			Mark_Inactive as "Mark Inactive"
			{
				type =  functions
				form = New_Product
				execution type = for each record	
				on start
				{
					actions 
					{
					on click
					(
						input.Active_Product = false;
					)
					}
				}
			}
			Mark_Active as "Mark Active"
			{
				type =  functions
				form = New_Product
				execution type = for each record	
				on start
				{
					actions 
					{
					on click
					(
						input.Active_Product = true;
					)
					}
				}
			}
			Change_Status as "Change Status"
			{
				type =  functions
				form = Add_Sales_Rep
				execution type = for each record	
				on start
				{
					actions 
					{
					on click
					(
						if(input.Active == true)
										{
											input.Active = false;
										}
										else
										{
											input.Active = true;
										}
					)
					}
				}
			}
			Approve as "Approve"
			{
				type =  functions
				form = New_Order
				execution type = collection of records	
				on start
				{
					actions 
					{
					on click
					(
						input.Status = "Approved";
					)
					}
				}
			}
			Reject as "Reject"
			{
				type =  functions
				form = New_Order
				execution type = collection of records	
				on start
				{
					actions 
					{
					on click
					(
						input.Status = "Rejected";
					)
					}
				}
			}
			Send_Invoice as "Send Invoice"
			{
				type =  functions
				form = New_Order
				execution type = for each record	
				on start
				{
					actions 
					{
					on click
					(
						contactDetails = New_Contact[Account == input.Account_Name] sort by Added_Time range from 0 to 1;
										if(contactDetails.ID != null)
										{
											invoiceUrl = "https://creatorapp.zoho.in" + zoho.appuri + "#View:My_Pending_Sale_Orders?Order_ID=" + input.Order_ID;
											sendmail
											[
												from :zoho.adminuserid
												to :contactDetails.Email
												subject :"Your Invoice for Order Id(" + input.Order_ID + ")"
												message :"Hi,<br>\n<br>\nYou can view your Invoice by access this <a href='" + invoiceUrl + "'>Invoice link.</a><br>\n<br>\nThanks!"
											]
										}
										else
										{
											info "There is no primary contact in this account!";
										}
					)
					}
				}
			}
			Unfollow_Leads as "Unfollow Leads"
			{
				type =  functions
				form = New_Lead
				execution type = for each record	
				on start
				{
					actions 
					{
					on click
					(
						empRow = Add_Sales_Rep[Email == zoho.loginuserid];
										if(empRow.ID != null)
										{
											if(input.Followers.contains(empRow.ID))
											{
												temp = input.Followers;
												temp.removeelement(empRow.ID);
												input.Followers = temp;
											}
										}
										else
										{
											info "Your Email Id is not registered in Employees Form!";
										}
					)
					}
				}
			}
