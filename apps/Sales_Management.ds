	section Add_Product
	{
		displayname= "Products"
		icon = "shopping-box"
		form New_Product
		{
			displayname = "New Product"
			success message = "Data Added Successfully!"
			field alignment = left
			feature property = 32
			Section
			(
				type = section
	 			row = 1
	 			column = 0   
				width = medium
			)
			must have Product_Group
			(
				type = picklist	
				displayname = "Product Group"
				values  = Add_Product_Group.ID
    			displayformat = [Product_Group]
				allow new entries = true
				[
					displayname = "Add New Product Group"
				]
				sortorder = ascending
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have Product_Name
			(
    			type = text
				displayname = "Product Name"
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have unique Product_Code
			(
    			type = text
				displayname = "Product Code"
				qrcode = true
				barcode = true
				camera = primary
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have Unit_Price
			(
				displayname = "Unit Price"
				type = INR
	 			row = 1
	 			column = 1   
				width = medium
			)
			Description
			(
    			type = textarea
				height = 100px
	 			row = 1
	 			column = 1   
				width = medium
			)
			Active_Product
			(
    			type = checkbox
				displayname = "Active Product"
				initial value = true
	 			row = 1
	 			column = 1   
				width = medium
			)
	
			customize
			(
				icon = "design-web-design"
			)
			actions
			{
				on add
				{
					submit
					(
   						type = submit
   						displayname = "Submit"
					)
					reset
					(
   						type = reset
   						displayname = "Reset"
					)
				}
				on edit
				{
					update
					(
   						type = submit
   						displayname = "Update"
					)
					cancel
					(
   						type = cancel
   						displayname = "Cancel"
					)
				}
			}
		}
