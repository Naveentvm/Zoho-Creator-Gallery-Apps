			Run_Scripts_On_Always22 as "Run Scripts - On Always"
			{
				type =  form
				form = Detailed_Forecast
				record event = on add or edit

				on validate
				{
					actions 
					{
						custom deluge script
						(
											isProceed = true;
										if(input.Period_Type == "Year")
										{
											if(input.January_Total == null || input.February_Total == null || input.March_Total == null || input.April_Total == null || input.May_Total == null || input.June_Total == null || input.July_Total == null || input.August_Total == null || input.September_Total == null || input.October_Total == null || input.November_Total == null || input.December_Total == null)
											{
												isProceed = false;
											}
										}
										else if(input.Period_Type == "Quarter")
										{
											if(Quarter == "1")
											{
												if(input.January_Total == null || input.February_Total == null || input.March_Total == null)
												{
													isProceed = false;
												}
											}
											if(Quarter == "2")
											{
												if(input.April_Total == null || input.May_Total == null || input.June_Total == null)
												{
													isProceed = false;
												}
											}
											if(Quarter == "3")
											{
												if(input.July_Total == null || input.August_Total == null || input.September_Total == null)
												{
													isProceed = false;
												}
											}
											if(Quarter == "4")
											{
												if(input.October_Total == null || input.November_Total == null || input.December_Total == null)
												{
													isProceed = false;
												}
											}
										}
										else if(input.Period_Type == "Month")
										{
											if(input.Week1_Total == null || input.Week2_Total == null || input.Week3_Total == null || input.Week4_Total == null || input.Week5_Total == null)
											{
												isProceed = false;
											}
										}
										else
										{
											if(input.Monday_Total == null || input.Tuesday_Total == null || input.Wednesday_Total == null || input.Thursday_Total == null || input.Friday_Total == null || input.Saturday_Total == null || input.Sunday_Total == null)
											{
												isProceed = false;
											}
										}
										if(input.Year_field == null || input.Year_field == "")
										{
											alert "Please enter the Year!";
											cancel submit;
										}
										if(isProceed == false)
										{
											alert "Please enter all the details!";
											cancel submit;
										}
						)
					}
				}

			}
