		{
			displayName = "Product Details"
			show all rows from New_Product    
			(
				Product_Group as "Product Group"
				Product_Name as "Product Name"
				Product_Code as "Product Code"
				Unit_Price as "Unit Price"
				Description
				Active_Product as "Active Product"
				custom action "Mark Inactive"
				(
	  			workflow = Mark_Inactive
				column header = "Mark Inactive"
				condition = (Active_Product == true)
				)
				custom action "Mark Active"
				(
	  			workflow = Mark_Active
				column header = "Mark Active"
				condition = (Active_Product == false)
				)
			)
			filters
			(
				Product_Group
			)
			group by
			(
				Product_Group ascending
			)
			options
			(
				icon = "users-2-contacts-44"
	 		)
			conditional formatting
			(
				"Available Product"
				{
				condition = (Active_Product == true)
				fields = [Product_Name]
				format = "color:#3dbf87;"
				}
				"Not Available"
				{
				condition = (Active_Product == false)
				fields = [Product_Name]
				format = "color:#bf2626;"
				}
			)
