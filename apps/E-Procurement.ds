		form Create_New_Procurement_Plan
		{
			displayname = "Create New Procurement Plan"
			success message = "Data Added Successfully!"
			field alignment = left
			feature property = 32
			Section
			(
				type = section
	 			row = 1
	 			column = 0   
				width = medium
			)
			Request_ID
			(
    			type = autonumber
				displayname = "Request ID"
				start index = 1
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have Request_By
			(
				type = picklist	
				displayname = "Request By"
				values  = Add_User.ID
    			displayformat = [" " + Name.first_name + " " + Name.last_name]
				sortorder = ascending
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have Requested_Date
			(
    			type = date
				displayname = "Requested Date"
				alloweddays = 0,1,2,3,4,5,6
	 			row = 1
	 			column = 1   
				width = medium
			)
			Expected_Date
			(
    			type = date
				displayname = "Expected Date"
				alloweddays = 0,1,2,3,4,5,6
	 			row = 1
	 			column = 1   
				width = medium
			)
			Schedule_Date
			(
    			type = date
				displayname = "Schedule Date"
				alloweddays = 0,1,2,3,4,5,6
	 			row = 1
	 			column = 1   
				width = medium
			)
			Already_Discussed_With_Manager
			(
    			type = checkbox
				displayname = "Already Discussed With Manager"
				initial value = false
	 			row = 1
	 			column = 2   
				width = medium
			)
			must have Manager
			(
				type = picklist	
				values  = Add_User[Role == "Manager"].ID
    			displayformat = [" " + Mail_Id]
				sortorder = ascending
	 			row = 1
	 			column = 2   
				width = medium
			)
			Approver
			(
    			type = picklist
				module = users
				displayformat = [username]
	 			row = 1
	 			column = 2   
				width = medium
				personal data = true
			)
			must have Items
			(
				type = grid
				defaultrow = 1
				Product
				(
					type = picklist	
					values  = Add_Product.ID
    				displayformat = [Product_Name + " - " + Brand + " - " + Size]
					allow new entries = true
					[
						displayname = "Add New Product"
					]
					sortorder = ascending
					width = medium
				)
				must have Quantity
				(
					type = number
					initial value = 1
					width = medium
				)
	 			row = 2
	 			column = 1   
