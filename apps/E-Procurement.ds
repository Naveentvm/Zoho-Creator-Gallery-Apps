			Rating_Calculation as "Rating Calculation"
			{
				type =  form
				form = Order_Fulfillment_Form
				button = Submit
				on click
				{
					actions 
					{
					on click
					(
						ratings = {"Very Unsatisfied":1,"Unsatisfied":2,"Neutral":3,"Satisfied":4,"Very satisfied":5};
										rate = (ratings.get(input.Delivery) + ratings.get(input.Quality) + ratings.get(input.Price) + ratings.get(input.Service) + ratings.get(input.Risk) + ratings.get(input.Support)) / 6;
										input.Overal_Ratings = rate.round(1);
										order = Purchase_Order[Order_ID == input.Order_ID];
										order.Order_Rating=input.Overal_Ratings;
										vendor_ratings = Purchase_Order[Vendor == order.Vendor.ID].avg(Order_Rating);
										vendor = Add_Vendor[ID == order.Vendor.ID];
										vendor.Overal_Ratings=vendor_ratings;
										if(input.Order_Status == "Finished")
										{
											order.Status="Finished";
										}
										else
										{
											order.Status="Denied";
										}
					)
					}
				}
			}
		}

		schedule
		{
			Reminder_Mail as "Reminder Mail"
			{
				type =  schedule
				form = Create_New_Procurement_Plan[Approval_Status == "Scheduled"]
				start = before 1 days from Schedule_Date at "08:00:00"
				end   = Schedule_Date
				frequency = daily
				time zone = "Asia/Kolkata"
				on start
				{
					actions  (Schedule_Date is tomorrow)
					{
					on load
					(
						toMail = input.Request_By.Mail_Id;
										sub = "Procurement Reminder";
										msg = "Your request for the procure the products has been scheduled tomorrow";
										thisapp.commonFunctions.sendMail(toMail,sub,msg);
					)
					}
					actions  (Schedule_Date is today)
					{
					on load
					(
						toMailID = input.Request_By.Mail_Id;
										sub = "Procurment: Reminder Mail";
										msg = "Your procurment request has been scheduled today. You can proceed the next lever now.";
										thisapp.commonFunctions.sendMail(toMailID,sub,msg);
					)
					}
				}
			}
		}

