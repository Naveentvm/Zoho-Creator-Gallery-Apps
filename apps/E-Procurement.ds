				 void commonFunctions.add_Remark(int rec_ID)
         {
         	openUrl(zoho.appuri + "#Form:Remark_Stateless_Form?Purchase_requisition=" + rec_ID,"popup window");
         }
				 float commonFunctions.findRating(list:string remarks)
         {
         	rate_configuration = {"Very Unsatisfied":1,"Unsatisfied":2,"Neutral":3,"Satisfied":4,"Very Satisfied":5};
         	rattings = List();
         	for each  remark in remarks
         	{
         		rattings.add(rate_configuration.get(remark));
         	}
         	return rattings.average();
         }
