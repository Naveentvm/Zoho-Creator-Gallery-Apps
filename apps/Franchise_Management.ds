		 functions
		 {
			 Deluge
			 {
				 void CustomAction.statusUpdate(int recID)
         {
         	openUrl("#Form:Status_Update?Purchase_ID=" + recID,"popup window");
         }
			 }
		 }




		workflow
		{
		form
		{
			Run_Scripts_On_Click_of_U as "Run Scripts - On Click of 'Update' Button"
			{
				type =  form
				form = Status_Update
				button = Update
				on click
				{
					actions 
					{
					on click
					(
						purchaseDetails = Purchase_Order[ID == input.Purchase_ID];
										if(input.Status == "Delivered")
										{
											//Checking the product availablity
											stockAvailable = true;
											for each  eachRequestedProduct in purchaseDetails.Products
											{
												productDetails = Add_Product[ID = eachRequestedProduct.Product];
												if(productDetails.Available_Stocks < eachRequestedProduct.Quantity)
												{
													stockAvailable = false;
												}
											}
											if(stockAvailable)
											{
												//Updating the purchase order status here
												purchaseDetails.Status=input.Status;
												purchaseDetails.Status_Updated_On=zoho.currentdate;
												purchaseDetails.Comments=input.Remarks;
												//Updating the stock availability
												for each  eachRequestedProduct in purchaseDetails.Products
												{
													productDetails = Add_Product[ID = eachRequestedProduct.Product];
													productDetails.Available_Stocks=productDetails.Available_Stocks - eachRequestedProduct.Quantity;
												}
												openUrl("#Script:page.refresh","same window");
											}
											else
											{
												alert "Requested product quantity is not available.";
											}
										}
										else if(input.Status == "Declined")
