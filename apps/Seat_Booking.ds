			Run_Scripts_While_Adding_2 as "Run Scripts - While Adding a Record"
			{
				type =  form
				form = Pick_Your_Seat
				record event = on add

				on load
				{
					actions 
					{
						custom deluge script
						(
											if(Add_Details[ID != 0].count() == 0)
