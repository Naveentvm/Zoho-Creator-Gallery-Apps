				form = Add_Employee
				record event = on add

				on load
				{
					actions 
					{
						custom deluge script
						(
											//Dynamically adding managers list to 'Reporting To' field
										if(Add_Employee.count() == 0)
										{
											input.Official_Email = zoho.loginuserid;
											disable Official_Email;
										}
										managersList = Add_Employee[Is_a_Reporting_Manager == true].Official_Email.getAll();
										input.Reporting_To:ui.add(managersList);
						)
					}
				}

			}
			Create_Record_Scripts6 as "Create Record Scripts"
			{
				type =  form
				form = Add_Employee
				record event = on add

				on success
				{
					actions 
					{
						custom deluge script
						(
											if(input.Is_a_Reporting_Manager == true)
										{
											thisapp.permissions.assignUserInProfile(input.Official_Email,"Manager");
										}
										else if(input.Is_a_Reporting_Manager == false)
										{
											thisapp.permissions.assignUserInProfile(input.Official_Email,"Employee");
										}
										getCompanyDetails = Add_Organization[ID != 0];
										sendmail
										[
											from :zoho.adminuserid
											to :input.Official_Email
											subject :"Invitation to access Employee Management"
											message :("<div>Hi " + input.Name) + ",</div><br><div> You have been added as an employee in " + getCompanyDetails.Organization_Name + ".</div> <br> <div> Click <a href=\"https://app.zohocreator.in" + zoho.appuri + "\"> here </a> to access the employee portal.</div> <br><div> Happy Working! </div> <br> <div> Regards,</div><br><div>" + zoho.adminuser + "</div>"
										]
										if(Add_Employee.count() == 0)
										{
											openUrl(zoho.appuri + "#Page:Employee_Profile","same window");
										}
						)
					}
				}

			}
			Create_Record_Scripts7 as "Create Record Scripts"
			{
				type =  form
				form = Salary_Structure
				record event = on add

				on load
				{
					actions 
					{
						custom deluge script
						(
											disable Salary_Split_Up.Annual;
						)
					}
				}

			}
			Field_Scripts7 as "Field Scripts"
			{
				type =  form
				form = Salary_Structure
				record event = on add or edit

				on user input of Salary_Split_Up.Monthly
				{
					actions 
					{
						custom deluge script
						(
											//Autocalculate annual pay
										if(row.Monthly != null)
										{
											row.Annual=row.Monthly * 12;
										}
										else
										{
											row.Annual=0;
										}
						)
					}
				}

			}
			Run_Scripts_while_editing1 as "Run Scripts - while editing Record"
			{
				type =  form
				form = Add_Employee
				record event = on edit

				on validate
				{
					actions 
					{
						custom deluge script
						(
											if(old.Official_Email != input.Official_Email)
										{
											thisapp.permissions.deleteUser(old.Official_Email);
											if(input.Is_a_Reporting_Manager == true)
											{
												thisapp.permissions.assignUserInProfile(input.Official_Email,"Manager");
											}
											else if(input.Is_a_Reporting_Manager == false)
											{
												thisapp.permissions.assignUserInProfile(input.Official_Email,"Employee");
											}
											getCompanyDetails = Add_Organization[ID != 0];
											sendmail
											[
												from :zoho.adminuserid
												to :input.Official_Email
												subject :"Invitation to access Employee Management"
												message :("<div>Hi " + input.Name) + ",</div><br><div> You have been added as an employee in " + getCompanyDetails.Organization_Name + ".</div> <br> <div> Click <a href=\"https://app.zohocreator.in" + zoho.appuri + "\"> here </a> to access the employee portal.</div> <br><div> Happy Working! </div> <br> <div> Regards,</div><br><div>" + zoho.adminuser + "</div>"
											]
										}
										else if(old.Is_a_Reporting_Manager != input.Is_a_Reporting_Manager)
										{
											if(input.Is_a_Reporting_Manager == true)
											{
												thisapp.permissions.assignUserInProfile(input.Official_Email,"Manager");
											}
											else if(input.Is_a_Reporting_Manager == false)
											{
												thisapp.permissions.assignUserInProfile(input.Official_Email,"Employee");
											}
										}
						)
					}
				}

			}
