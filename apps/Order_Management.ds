				start = before 1 days from Estimated_Delivery_Date at "09:00:00"
				time zone = "Asia/Kolkata"
				on start
				{
					actions 
					{
					on load
					(
						sendmail
										[
											from :zoho.adminuserid
											to :input.Sales_Owner.Email
											subject :"[Order ID-" + input.Order_ID + "]Delivery Reminder"
											message :"Hi,<div><br></div><div>This is a gentle reminder to deliver this [" + input.Order_ID + "]order by tomorrow.</div><div><br></div><div>Kindly process this as soon as possible.</div><div><br></div><div>Regards,</div><div>" + zoho.adminuser + "</div><div><br></div>"
										]
					)
					}
				}
			}
		}



