	section New_Order
	{
		displayname= "Orders"
		form New_Order
		{
			displayname = "New Order"
			success message = "New order has been placed!"
			field alignment = left
			feature property = 32
			Section
			(
				type = section
	 			row = 1
	 			column = 0   
				width = medium
			)
			Order_ID
			(
    			type = autonumber
				displayname = "Order ID"
				start index = 1
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have Sales_Owner
			(
				type = picklist	
				displayname = "Sales Owner"
				values  = Add_Employee.ID
    			displayformat = [Employee_ID + " - " + Name.first_name + Name.last_name]
				sortorder = ascending
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have Customer
			(
				type = picklist	
				values  = Add_Customer.ID
    			displayformat = [Customer_ID + " - " + Name.first_name + " " + Name.last_name]
				allow new entries = true
				[
					displayname = "Add New Customer"
				]
				sortorder = ascending
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have Order_Date
			(
    			type = date
				displayname = "Order Date"
				alloweddays = 0,1,2,3,4,5,6
	 			row = 1
	 			column = 1   
				width = medium
			)
			Delivery_Agency
			(
				type = picklist	
				displayname = "Delivery Agency"
				values  = Add_Delivery_Agency.ID
    			displayformat = [Agency_Name]
				sortorder = ascending
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have Estimated_Delivery_Date
			(
    			type = date
				displayname = "Estimated Delivery Date"
				alloweddays = 0,1,2,3,4,5,6
	 			row = 1
	 			column = 1   
				width = medium
			)
			Actual_Delivery_Date
			(
    			type = date
				displayname = "Actual Delivery Date"
				alloweddays = 0,1,2,3,4,5,6
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have Products
			(
				type = grid
				values  = Orders_Details.ID
    			bidirectional = Order_ID
	 			row = 2
	 			column = 1   
				width = medium
			)
			Section1
			(
				type = section
				displayname = "Section"
	 			row = 3
	 			column = 0   
				width = medium
			)
			Sub_Total
			(
				displayname = "Sub Total"
				type = INR
				initial value = 0.00
	 			row = 3
	 			column = 1   
				width = medium
			)
			must have Tax
			(
    			type = percentage
				initial value = 0%
	 			row = 3
	 			column = 1   
				width = medium
			)
			Net_Total
			(
				displayname = "Net Total"
				type = INR
				initial value = 0.00
	 			row = 3
	 			column = 1   
				width = medium
			)
			Reason_For_Cancellation
			(
    			type = textarea
				displayname = "Reason For Cancellation"
				height = 100px
	 			row = 3
	 			column = 1   
				width = medium
			)
			Status
			(
				type = picklist
				values = {"Pending","Ready For Delivery","Delivered","Cancelled"}
				initial value = "Pending"
	 			row = 3
	 			column = 1   
				width = medium
			)
	
			actions
			{
				on add
				{
					submit
					(
   						type = submit
   						displayname = "Submit"
					)
					reset
					(
   						type = reset
   						displayname = "Reset"
					)
				}
				on edit
				{
					update
					(
   						type = submit
   						displayname = "Update"
					)
					cancel
					(
   						type = cancel
   						displayname = "Cancel"
					)
				}
			}
		}
		list Pending_Orders
		{
			displayName = "Pending Orders"
			show all rows from New_Order  [Status == "Pending"]  
			(
				Products
				(
					field = Products.Order_ID + " " + Products.Product
				)
				Reason_For_Cancellation as "Reason For Cancellation"
