		workflow
		{
		form
		{
			Assign_Actions as "Assign"
			{
				type =  form
				form = Select_Assignees
				button = Assign
				on click
				{
					actions 
					{
					on click
					(
						thisapp.assign(input.New_Bug_RecID.toLong(),input.Select_Assignees1);
										openUrl("#Script:page.reload ","same window");
					)
					}
				}
			}
			Change_Actions as "Change"
			{
				type =  form
				form = Change_Status_PopUp
				button = Change
				on click
				{
					actions 
					{
					on click
					(
						rec = New_Bug[ID == input.Bug_RecID.toLong()];
										rec.Status=input.Status;
										rec.Comment=input.Comment;
										userRec = Add_User[Email == zoho.loginuserid];
										if(rec.Assignees1 == userRec.ID)
										{
											user = "Assignee";
										}
										else if(rec.Reporter == userRec.ID)
										{
											user = "Reporter";
										}
										insert into Comments
										[
											Added_User=zoho.loginuser
											Submitted_Bug_Rec_ID=input.Bug_RecID.toLong()
											Comment=input.Comment
											Commenter=userRec.ID
											User=user
										]
										if(input.Status == "Reopened")
										{
											sendmail
											[
												from :zoho.adminuserid
												to :rec.Assignees1.Email.toString()
												subject :"Bug Reopened"
												message :"An bug has been reopened after testing, please fix it."
											]
										}
										else if(input.Status == "Fixed")
										{
											sendmail
											[
												from :zoho.adminuserid
												to :rec.Reporter.Email.toString(),zoho.adminuserid
												subject :"Bug Fixed"
												message :"An bug has been Fixed."
											]
										}
										else if(input.Status == "Open")
										{
											sendmail
											[
												from :zoho.adminuserid
												to :rec.Reporter.Email.toString()
												subject :"Bug Assigned"
												message :"An bug has been Assigned to you.<div><br></div><div>Please fix it and update.</div>"
											]
										}
										else if(input.Status == "Rejected")
										{
											sendmail
											[
												from :zoho.adminuserid
												to :rec.Reporter.Email.toString()
												subject :"Bug Rejected"
												message :"An bug has been Rejected."
											]
										}
										else if(input.Status == "Not Reproducible")
										{
											sendmail
											[
												from :zoho.adminuserid
												to :rec.Reporter.Email.toString()
												subject :"Bug Not Reproducible"
												message :"An bug has been marked as Not Reproducible."
											]
										}
										else if(input.Status == "Not Fixing Now")
										{
											sendmail
											[
												from :zoho.adminuserid
												to :rec.Reporter.Email.toString()
												subject :"Bug Not Fixing Now"
												message :"An bug has been marked as Not Fixing Now."
											]
										}
										else if(input.Status == "Review/Testing")
										{
											sendmail
											[
												from :zoho.adminuserid
												to :rec.Reporter.Email.toString()
												subject :"Bug Sent for Review"
												message :"An bug has been sent back for Reviewing/Testing."
											]
										}
										openUrl("#Script:page.reload ","same window");
					)
					}
				}
			}
