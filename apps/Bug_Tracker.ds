				 void addComment(int id)
         {
         	openUrl("#Form:Comments?Submitted_Bug_Rec_ID=" + id,"popup window");
         }
				 void assign(int id, list assigneesList)
         {
         	rec = New_Bug[ID == input.id];
         	rec.Assignees1=assigneesList;
         	rec.Status="Open";
         	assigneesEmail = Add_User[ID in input.assigneesList].Email.getAll();
         	sendmail
         	[
         		from :zoho.adminuserid
         		to :assigneesEmail.toString()
         		subject :"New Bug Assigned."
         		message :("Hello, <br><br>New bug has been assigned to you, which is reported by&nbsp;" + rec.Reporter.Name1) + " on " + rec.Added_Time.toDate() + ".<div><br></div><div>Please fix it before " + rec.Deadline.toDate() + "</div><div><br></div><div>Thank You.</div>"
         	]
         }
				 void changeStatus(int id)
         {
         	openUrl("#Form:Change_Status_PopUp?Bug_RecID=" + id,"popup window");
         }
				 void openSelectAssigneeForm(int id)
         {
         	openUrl("#Form:Select_Assignees?New_Bug_RecID=" + id,"popup window");
         }
				 void viewComments(int id)
         {
         	openUrl("#Page:Comment_View?id=" + id,"popup window");
         }
			 }
