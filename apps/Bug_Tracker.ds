		schedule
		{
			Deadline_Reminder as "Deadline_Reminder"
			{
				type =  schedule
				form = New_Bug
				start = before 1 days from Deadline at "10:30:00"
				time zone = "Asia/Kolkata"
				on start
				{
					actions 
					{
					on load
					(
						if(input.Status != "Fixed")
										{
											assigneesEmails = Add_User[ID in input.Assignees1].Email.getAll();
											for each  assgign in assigneesEmails
											{
												assigneesName = Add_User[Email == assgign && ID in input.Assignees1].Name1;
												sendmail
												[
													from :zoho.adminuserid
													to :assgign.toString()
													subject :"Bug Deadline Reminder"
													message :("Hi " + assigneesName) + "<br><br>Deadline for bug-ID" + input.Bug_ID + " is tomorrow. This is gentle reminder, please fix it."
												]
											}
										}
					)
					}
				}
			}
		}



