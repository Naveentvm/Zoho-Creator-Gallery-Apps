			Run_Scripts_On_Always7 as "Run Scripts On Always"
			{
				type =  form
				form = Set_Reminder
				record event = on add or edit

				field rules
				{
					actions 
					{
						hide Next_Reminder_On;
					}
					actions  (Run_This_Schedule == "Number Of Events")
					{
						show Number_Of_Events;
					}
					actions  (Run_This_Schedule == "Until a Date")
					{
						show End_Date;
					}
					actions  (Frequency != "Once")
					{
						show Run_This_Schedule;
					}
				}

			}
			Run_Scripts_On_Always8 as "Run Scripts On Always"
			{
				type =  form
				form = Set_Reminder
				record event = on add or edit

				on validate
				{
					actions 
					{
						custom deluge script
						(
											if(Starting_Time <= zoho.currenttime)
										{
											alert "Enter a valid future starting date and time";
											cancel submit;
										}
										if(Frequency == "Once" && Run_This_Schedule != "")
										{
											input.Run_This_Schedule = "";
										}
										if(Run_This_Schedule == "Forever" && End_Date != null)
										{
											input.End_Date = null;
										}
						)
					}
					actions  (Run_This_Schedule == "Number Of Events")
					{
						custom deluge script
						(
											if(Number_Of_Events == null)
										{
											alert "Enter the number of events to complete this schedule";
											cancel submit;
										}
										else if(input.Number_Of_Events <= 0)
										{
											alert "Enter a valid number for events";
											cancel submit;
										}
										else
										{
											value_map = {"Daily":input.Starting_Time.addDay(input.Number_Of_Events),"Monthly":input.Starting_Time.addMonth(input.Number_Of_Events),"Weekly":input.Starting_Time.addWeek(input.Number_Of_Events),"Yearly":input.Starting_Time.addYear(input.Number_Of_Events)};
											input.End_Date = value_map.get(input.Frequency).toDate();
										}
						)
					}
					actions  (Run_This_Schedule == "Until a Date")
					{
						custom deluge script
						(
											if(End_Date == null)
										{
											alert "Enter a valid end date to finish this schedule";
											cancel submit;
										}
										else if(End_Date <= input.Starting_Time.toDate())
										{
											alert "End date should be greater than starting date";
											cancel submit;
										}
										if(input.Number_Of_Events != null)
										{
											input.Number_Of_Events = null;
										}
						)
					}
					actions  (Frequency != "Once")
					{
						custom deluge script
						(
											if(Run_This_Schedule == "" || Run_This_Schedule == null)
										{
											alert "Select any format to run this schedule";
											cancel submit;
										}
						)
					}
				}

			}
