				 void Employee.Changing_Employee_Status(int record_id)
         {
         	//Changing Status
         	this_record = Add_Employee[ID == input.record_id];
         	if(this_record.Employee_Status == "Active")
         	{
         		work_count = Request_For_Maintenance[Assigned_Employee == this_record.ID && Work_Status != "Closed"].count(ID);
         		if(work_count == 0)
         		{
         			this_record.Employee_Status="Archive";
         		}
         		//Alerts the pending works of the employee
         		else
         		{
         			info "This Employee has " + work_count + " pending works to do ,So please change the status after the works completed";
         		}
         	}
         	else if(this_record.Employee_Status == "Archive")
         	{
         		this_record.Employee_Status="Active";
         	}
         }
				 void Maintenance.Assign_Employee(int record_id)
         {
         	//Redirects to the employee assigning form
         	url = zoho.appuri + "#Form:Assign_Employee?Issue_Title=" + record_id;
         	openUrl(url,"popup window");
         }
				 void Maintenance.Closing_Request(int record_id)
         {
         	//Changing work status to closed and sent an email to the requester about the work completion
         	this_record = Request_For_Maintenance[ID == input.record_id];
         	this_record.Work_Status="Closed";
         	requester_name = Add_Requester[ID == this_record.Requester_Name].Name;
         	requester_email_id = Add_Requester[ID == this_record.Requester_Name].Email_Id;
         	url = "https://app.zohocreator.in" + zoho.appuri + "#Form:Feedback";
         	sendmail
         	[
         		from :zoho.adminuserid
         		to :requester_email_id
         		subject :"Maintenance Closed"
         		message :("Hi " + requester_name) + ",<br></br>Your requested maintenace for " + this_record.What_s_The_Issue + "was closed.<br></br><a href=" + url + ">Click here to give your feedback</a>"
         	]
         }
				 void Maintenance.Moving_Request_To_Progress(int record_id)
         {
         	//Changing the work status as In progress
         	this_record = Request_For_Maintenance[ID == input.record_id];
         	this_record.Work_Status="In progress";
         }
				 void Notification.Send_Notification(string userEmail)
         {
         	appname = zoho.appname;
         	sendmail
         	[
         		from :zoho.adminuserid
         		to :userEmail
         		subject :"Invitation to access '" + zoho.appname + "' "
         		message :"<div style='width:100%;display:inline-block;box-sizing:border-box;border:1px solid #ddd;'><div style='padding:2%;text-align:center;'>You've been invited to use an application built on Zoho Creator by \"" + zoho.adminuserid + "\".</div><div style='background: #efefef; padding: 20.0px;margin: 20.0px 0 10.0px;text-align:center;'><h2>" + appname.toUpperCase() + "</h2><div style='margin: 20.0px 0 10.0px;'><a style='padding:10px;background-color: rgb(62,149,205);text-decoration: none;color: rgb(255,255,255);font-size: 14.0px;' href='https://creator.zoho.in" + zoho.appuri + "' target='_blank'> Go to application </a></div></div></div>"
         	]
         }
			 }
