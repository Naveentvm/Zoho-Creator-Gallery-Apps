			displayname = "Request For Maintenance"
			success message = "Request has been sent successfully"
			field alignment = left
			feature property = 32
			Section
			(
				type = section
	 			row = 1
	 			column = 0   
				width = medium
			)
			must have Date_field
			(
    			type = date
				displayname = "Date"
				alloweddays = 0,1,2,3,4,5,6
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have Requester_Name
			(
				type = picklist	
				displayname = "Requester Name"
				values  = Add_Requester.ID
    			displayformat = [" " + Name.first_name + Name.last_name]
				sortorder = ascending
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have Select_Service
			(
				type = picklist	
				displayname = "Select Service"
				values  = Add_Service.ID
    			displayformat = [Service_Name]
				sortorder = ascending
    			description
    			[
    				type = tooltip
    			 	message = "If your expected service is not here then please contact admin"
    			]
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have What_s_The_Issue
			(
    			type = text
				displayname = "What's The Issue?"
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have Description
			(
    			type = textarea
				height = 100px
	 			row = 1
	 			column = 1   
				width = medium
			)
