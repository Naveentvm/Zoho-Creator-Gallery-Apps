		{
			displayname = "Add Employee"
			success message = "Employee has been added successfully"
			field alignment = left
			feature property = 32
			Section
			(
				type = section
	 			row = 1
	 			column = 0   
				width = medium
			)
			must have Service
			(
				type = picklist	
				values  = Add_Service.ID
    			displayformat = [Service_Name]
				allow new entries = true
				[
					displayname = "Add New Service"
				]
				sortorder = ascending
	 			row = 1
	 			column = 1   
				width = medium
			)
			must have Employee_Name
			(
    			type = name
				displayname = "Employee Name"
     			prefix
     			(
	     			 type = prefix
	     			 displayname ="Prefix"
